<section local-class="profile">
  <div>
    {{#each image as |item|}}
      <div local-class="wrapper">
        {{ui-icon 36 36 type='image' image=item.image}}
      </div>
    {{/each}}
  </div>

  <div>
    {{#each image as |item|}}
      <div local-class="wrapper">
        {{ui-icon 48 48 type='image' image=item.image radius='4'}}
      </div>
    {{/each}}
  </div>

  <div>
    {{#each image as |item|}}
      <div local-class="wrapper">
        {{ui-icon 64 64 type='image' image=item.image radius='12'}}
      </div>
    {{/each}}
  </div>
</section>
<div class="code">
  <span>\{{ui-icon</span> <i>36</i> <i>36</i> type=<b>'image'</b> image=<u>'IMAGE'</u><span>}}</span>
  <span>\{{ui-icon</span> <i>48</i> <i>48</i> type=<b>'image'</b> image=<u>'IMAGE'</u> radius=<i>'4'</i><span>}}</span>
  <span>\{{ui-icon</span> <i>64</i> <i>64</i> type=<b>'image'</b> image=<u>'IMAGE'</u> radius=<i>'12'</i><span>}}</span>
</div>