{{input type=type name=name checked=checked value=value required=required readonly=readonly disabled=disabled}}
<label>
  {{#if switch}}
  {{else}}
    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="18px" height="18px" viewBox="0 0 18 18">
      <g><circle cx="9" cy="9" r="4"></circle></g>
    </svg>
  {{/if}}
  <span>{{label}}</span>
</label>
